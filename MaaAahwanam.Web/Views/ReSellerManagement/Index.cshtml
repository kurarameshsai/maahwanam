    <script>
        $('.mprice').on('input', function () {
            var table = $(this).parents('.tableprices');
            var rowindex = $(this).parent().index();
            var colindex = $(this).index();
            var saleprice = table.find("tr:eq(0)").find("td").eq(rowindex).find("input").val();
            var saleprice1 = table.find("tr:eq(3)").find("td").eq(rowindex).find("input").val();
            var type = table.find("tr:eq(1)").find("td").eq(rowindex).find("select").val();
            var fprice = '';
            if (type == "com") {
                var ksp = $(this).val();
                if (ksp > 100) { alert('Percentage should not be greater than 100'); } else {

                    fprice = saleprice - (saleprice * $(this).val() / 100);
                }
            } else if (type == "sfr") {
                var ksp = $(this).val();
                if (ksp > saleprice) { alert('Margin Amount should not be greater than Sale Price'); } else {

                    fprice = saleprice - $(this).val();
                }
            } else if (type == "comsfr") { fprice = (saleprice - (saleprice * $(this).val() / 100)) - saleprice1; }
            table.find("tr:eq(3)").find("td").eq(rowindex).find("input[type='number']").val(fprice);
        });
    </script>
