<style>
    .datetimepicker {
    }

    .card-body input[type=text], .card-body input[type="email"], .card-body input[type="tel"], .card-body input[type="number"] {
        width: 101%;
        padding: 13px 0px 2px 10px;
        margin: 15px 5px 15px 0px;
        box-sizing: border-box;
        background-color: #fff;
        color: grey;
    }

    .input__field {
        position: relative;
        display: block;
        float: right;
        padding: 1.1em;
        width: 101%;
        border: none;
        border-radius: 0;
        /*background: #f0f0f0;*/
        color: #aaa;
        font-weight: normal;
        font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
        -webkit-appearance: none;
    }

    .input__label--hoshi {
        position: absolute;
        bottom: 0;
        left: 0;
        padding: 5.7em 0.25em;
        width: 100%;
        height: calc(100% - 1em);
        text-align: left;
        pointer-events: none;
    }

    .ksc {
        margin-top: -13%;
    }

    .ksc1 {
        margin-top: -6%;
    }

    input[type="file"] {
        display: none;
    }

    .custom-file-upload {
        border: 1px solid #ccc;
        display: inline-block;
        /*padding: 6px 12px;*/
        cursor: pointer;
    }
</style>





