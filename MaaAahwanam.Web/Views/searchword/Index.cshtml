<script type="text/javascript">

    $(function () {
    $.ajax({
        type: "GET",
        url: '/searchword/AutoCompleteCountry',
        success: function (data) {
            var availableTags = data.split(',');
            $("#name").autocomplete({
                source: availableTags
            });
        }
    });
});
$("#name").click(function () {
    $(".ui-helper-hidden").css("display", "block");
});





