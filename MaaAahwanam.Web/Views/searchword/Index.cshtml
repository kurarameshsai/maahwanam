	if(ScrollTop > 1226){
		$("#topicon").css("display","block");
		}else{
			$("#topicon").css("display","none");}
	});
//<!-------- Body Scroll Top ----------->

//<!-------- Category Scroll Top ----------->
$(window).on("scroll",function(){
	var scrollTop = $(this).scrollTop();
	if(scrollTop > 150){
		$('.search-categories').addClass(' fixed');
		$('.filters').css({"top":"70px"});
      } else {
          $('.search-categories').removeClass('fixed');
		  $('.filters').css({"top":"203px"});
      }
	  });
//<!-------- Category Scroll Top ----------->

/*------- Category Tabs -------------*/
$(".search-categories > ul > li").click(function(){
	var topId = $(this).attr("id").trim();
	$(".live-deals-search-results > .filters + div > div").each(function(){
		var rightId = $(this).attr("id").trim();
		if(topId == rightId){
			$(this).css({"display":"block"});
			}else {
				$(this).css({"display":"none"});}
		});
	});
/*------- Category Tabs -------------*/

