    <script>
        // Show / Hide div
        $(document).ready(function () {
            $(".slidingDiv").hide();
            $(".show_hide").show();
            $("body").not(".show_hide,.slidingDiv").click(function () {
                $(".slidingDiv").fadeOut("slow");
            });

            $(".show_hide").click(function (e) {
                $(".slidingDiv").fadeIn('slow');
                e.stopPropagation();
            });

        });
    </script>
