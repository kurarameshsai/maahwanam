    <script type="text/javascript">
        window.onload = function () {
            //particular div enabling based on condition
            var servicetype = document.getElementById("stype").value;
            if (servicetype == "Other") {
                $('#Calcdiv_Other').removeAttr("style");
                $('#Calcdiv_Other').css({ "display": "block" });
            }
            else if (servicetype == "Venue") {
                $('#Calcdiv_venue').removeAttr("style");
                $('#Calcdiv_venue').css({ "display": "block" });
            }
            else if (servicetype == "InvitationCard") {
                $('#Calcdiv_invitation').removeAttr("style");
                $('#Calcdiv_invitation').css({ "display": "block" });
                $('#dviAddressService').css({ "display": "none" });
            }
            else if (servicetype == "Catering") {
                $('#Calcdiv_Caterer').removeAttr("style");
                $('#Calcdiv_Caterer').css({ "display": "block" });
            }
            else if (servicetype == "Gifts") {
                $('#Calcdiv_Gift').removeAttr("style");
                $('#Calcdiv_Gift').css({ "display": "block" });
                $('#dviAddressService').css({ "display": "none" });
            }
            else {
                $('#Calcdiv_General').removeAttr("style");
                $('#Calcdiv_General').css({ "display": "block" });
            }
            //alert($('input[name=platetypevenue]:checked', '#myFormvenue').val());

            //performing calculations
            var b;
            var a = document.getElementById("txtquantityvenue").value;
            var b = document.getElementById("txtquantityGeneral").value;
            var c = document.getElementById("txtquantityGift").value;
            //alert(document.getElementById("vlGift").value);
            var type = $("#venuetype").val();
            if (type == 'Banquet Hall') {
                if (document.getElementById("vlvenue").checked) {
                    $("#pax").show();
                    document.getElementById("demovenue").innerHTML = document.getElementById("vlvenue").value * a;
                }
            }
            else {
                if (document.getElementById("venueservice").checked) {
                    $("#pax").hide();//alert(document.getElementById("vlvenue").value);
                    document.getElementById("demovenue").innerHTML = document.getElementById("venueservice").value;
                }
            }
            //if (document.getElementById("vlother").checked) {
            //    document.getElementById("demoother").innerHTML =  document.getElementById("vlother").value * a;
            //}
            if (document.getElementById("vlCatering").checked) {
                document.getElementById("demoCatering").innerHTML = document.getElementById("vlCatering").value * a;
            }
            if (document.getElementById("vlGift").checked) {
                document.getElementById("demoGift").innerHTML =  document.getElementById("vlGift").value * c ;
            }
            if (document.getElementById("vlinvitation").checked) {
                document.getElementById("demoinvitation").innerHTML = document.getElementById("vlinvitation").value * a;
            }
            //if (document.getElementById("vlGeneral").checked) {
            //    //alert('s');
            //    document.getElementById("demoGeneral").innerHTML =  document.getElementById("vlGeneral").value * b ;
            //}

            //var servicetype = document.getElementById("stype").value;





            var fromcart = '@Request.QueryString["p"]';
            var cid = '@Request.QueryString["cid"]';
            //alert(fromcart);
            if (fromcart == 'c') {
                document.getElementById("btn1").style.display = "None";
                document.getElementById("btn2").style.display = "None";
                var itemdetails = {};
                itemdetails.cid = cid;
                //alert(itemdetails.cid);
                $.ajax({
                    type: "POST",
                    url: window.location.href.split("/")[0] + '//' + window.location.href.split("/")[2] + '/DealInfo/getproductfromcart',
                    data: {},
                    data: JSON.stringify(itemdetails),
                    //data: sendInfo,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        //alert(data.attribute);
                        var page = window.location.search.split("=")[1].split("&")[0];
                        //alert(page);
                        if(page=="Venue")
                        {
                            document.getElementById("txtquantityvenue").value = data.Quantity;
                            document.getElementById("demovenue").innerHTML = data.TotalPrice;
                            document.getElementById("btn3").style.display = "block";
                        }
                        else if(page=="Catering")
                        {
                            document.getElementById("txtquantityCatering").value = data.Quantity;
                            document.getElementById("demoCatering").innerHTML = data.TotalPrice;
                            document.getElementById("btn3").style.display = "block";
                        }
                        document.getElementById(data.attribute).checked = true;

                    },
                    error: function (data) {
                        alert('error')
                    }
                });
            }
        };

        function Sethdnvalue(btn) {
            document.getElementById("btnname").value = btn;
            //alert(document.getElementById("btnname").value);
        }

        //for venues
        var radvenue = document.myFormvenue.platetypevenue;
        var prevvenue = null;var type = $("#venuetype").val();
        var rrvenue = null;
        if ("@Request.QueryString["par"]" == "Venue") {
