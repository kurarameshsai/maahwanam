<section>
    <div class="about_main"><input type="hidden" id="vendoravailabledates" value="@ViewBag.vendoravailabledates" />
        <div class="pageWidth">
            <input type="hidden" id="hdndealid" value="@Request.QueryString["did"]" />
            <div class="deal_info_main">
                <div class="deal-info">
                    <div class="deal-slider">
                        <div class="sp-loading">
                            <img src="../CSSWeb/images/sp-loading.gif" alt="">
                            <br>
                            LOADING IMAGES
                        </div>
                        @*<div class="zoomDiv">*@
                        <div class="sp-wrap">
                            @if (ViewBag.Imagelist != null)
                            {
                                foreach (var item in ViewBag.Imagelist)
                                {
                                    <a href="~/vendorimages/@item"> <img src="~/vendorimages/@item" alt="thumb"></a>
                                }
                            }
                            else if (ViewBag.Imagelist == null)
                            {
                                <a href="#" onclick="return false">
                                    <img src="~/noimages.png" />
                                </a>
                            }
                        </div>
                        @*</div>*@

                    </div>
                    <div class="deal-desc">
                        <div class="review-area">
                            <ul>
                                <li>
                                    <div class="rate" data-rate-value=4></div>
                                    &nbsp;
                                </li>
                                <li>
                                    <a href="#review" class="move_review text-green"><i class="ti-comment-alt text-green"></i> Reviews @ViewBag.Reviewlist.Count</a>
                                </li>
                                @if (ViewBag.Userloginstatus != 0 && ViewBag.usertype != "Admin")
                                {
                                    <li class="text-red">
                                        <a href="#writeReview" class="text-red popup-with-move-anim"><i class="ti-pencil-alt text-red"></i> Write a Review</a>
                                    </li>
                                }
                                else
                                {
                                    <li class="text-red">
                                        <a href="../Signin/Index" class="text-red"><i class="ti-pencil-alt text-red"></i> Write a Review</a>
                                    </li>
                                }
                            </ul>
                            

                            <!-- popup -->
                            <div data-role="popup" id="writeReview" class="zoom-anim-dialog mfp-hide small-dialog">
                                <div class="popup-box pop-content">
                                    <div class="popup-head">
                                        <h3>Write a Review</h3>
                                    </div>
                                    <!-- /popup-head -->

                                    <div class="popup-space popup-desc-space">
                                        <div class="dashboardList dashboardList-nospace">
                                            <div class="bidding_form">

                                                @using (Html.BeginForm("WriteaRiview", "DealInfo", FormMethod.Post, new { @class = "form-signin", @id = "register_form", @autocomplete = "off" }))
                                                {
                                                    <ul>
                                                        <li>
                                                            <label>Name</label>
                                                            @Html.TextBoxFor(model => model.Item2.FirstName, new { @id = "name", @name = "name", placeholder = "Name", required = "required" })
                                                        </li>
                                                        <li>
                                                            <label>Email ID <span>*</span></label>
                                                            @Html.TextBoxFor(model => model.Item2.EmailId, new { @id = "EmailId", @name = "EmailId", placeholder = "EmailId", required = "required" })
                                                        </li>
                                                    </ul>
                                                    <ul>
                                                        <li class="full-width">
                                                            <label>Review <span>*</span></label>
                                                            <input type="hidden" id="stvalue" name="Minvalue" value="10">
                                                            <input type="hidden" id="hmin" name="Minvalue" value="10">
                                                            @Html.HiddenFor(model => model.Item2.Service, new { @id = "Service", @name = "Service", required = "required", Value = @Model.Item1.ServicType })
                                                            @Html.HiddenFor(model => model.Item2.ServiceId, new { @id = "Review", @name = "Review", required = "required", Value = @Model.Item1.Id })
                                                            @Html.HiddenFor(model => model.Item2.Sid, new { @id = "Subid", @name = "Review", required = "required", Value = @ViewBag.Subvid })
                                                            @Html.TextAreaFor(model => model.Item2.Comments, new { @id = "Review", @name = "Review", placeholder = "Review", required = "required" })
                                                        </li>
                                                    </ul>

                                                    <ul>
                                                        <li>&nbsp;</li>
                                                        <li>
                                                            <button type="submit" name="submit" formaction="DealInfo/WriteaRiview?did=@Request.QueryString["did"]" class="btn btn-small btn-green float-right" value="submit">Submit <i class="ti-angle-double-right"></i></button>
                                                        </li>
                                                    </ul>
                                                }

                                            </div>
                                            <div class="clear"></div>
                                        </div>
                                    </div>


                                </div>
                            </div>
                            <!-- end popup -->

                        </div>
                        <div class="product_review" id="dviAddressService">
                            <h4>Location</h4>
