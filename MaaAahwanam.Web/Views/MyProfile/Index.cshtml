@model MaaAahwanam.Models.UserDetail
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/SharedWeb/DashBoardMaster.cshtml";
}

@section Navigation
{
    <div class="breads">
    <div class="breads_inner">
        <div class="pageWidth">
            <div class="title">
                <h3>My Profile</h3>
            </div>
            <div class="breadcrumbs">
                <ul>
                    @if (ViewBag.Type == "User")
                    {
                        <li><a href="~/index/Index">Home</a></li>
                    }
                    <li><i class="breads_arrow"></i></li>
                    <li><a href="../Dashboard">Dashboard</a></li>
                    <li><i class="breads_arrow"></i></li>
                    <li>My Profile</li>
                </ul>
            </div>
        </div>
    </div>
</div>
}
<div class="my-profile">
    <div class="profileInfo">
        <ul>
            <li><strong>First Name:</strong> @Model.FirstName</li>
            <li><strong>Last Name:</strong> @Model.LastName</li>
        </ul>
        <ul>
            <li><strong>Alternative EmailID:</strong> @Model.AlternativeEmailID</li>
            <li><strong>Mobile Number:</strong> @Model.UserPhone</li>
        </ul>
        <ul>
            <li><strong>Gender:</strong> @Model.Gender</li>
            <li><strong>Address:</strong>@Model.Address</li>
        </ul>
        <ul>
            <li><strong>Landmark:</strong> @Model.Landmark</li>
            <li><strong>City:</strong> @Model.City</li>
        </ul>
        <ul>
            <li><strong>State:</strong> @Model.State</li>
            <li><strong>Country:</strong> @Model.Country</li>
        </ul>
        <ul>
            <li><strong>URL:</strong> @Model.Url</li>
            <li><strong>Zip:</strong> @Model.ZipCode</li>
        </ul>

        <ul>
            <li class="fullDiv"><strong>Description:</strong> Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer pharetra lacinia tincidunt. Duis pretium dolor at risus iaculis, ac laoreet risus mollis. Sed at diam lacinia ligula facilisis convallis vel non sapien. Curabitur pellentesque lorem sit amet metus ornare, id molestie nisi dignissim. Nullam venenatis dolor bibendum, viverra nisi eu, cursus est. Praesent vitae arcu dui. In euismod pharetra lorem at malesuada. Proin ac nulla quis nisi scelerisque convallis. Nulla augue odio, luctus non ante sit amet, interdum eleifend libero.  </li>
        </ul>
    </div>
    <div class="clear"></div>
</div>

