@{
    ViewBag.Title = "TestimonialDetails";
    Layout = "~/Areas/Admin/Views/Shared/MainMaster.cshtml";
}
@section header{
    <!-- PAGE LEVEL STYLES -->
    <link href="~/Assets/plugins/dataTables/dataTables.bootstrap.css" rel="stylesheet" />
    <!-- END PAGE LEVEL  STYLES -->
}
<!--PAGE CONTENT -->
<div id="content">
    <div class="inner">
        <div class="row">
            <div class="col-lg-12" align="center">
                <h2> Testimonial Details</h2>
            </div>
        </div>
        <hr />
       
        <div class="row">
            <div class="col-lg-12">
                <div class="body collapse in" id="div1"> <a href="/Admin/Dashboard/dashboard" class="btn btn-primary btn-xs">Home</a>&nbsp;&nbsp;&nbsp;<a href="/Admin/Others/Testimonials" class="btn btn-primary btn-xs">Testimonials</a>&nbsp;&nbsp;&nbsp;<a href="#" class="btn btn-primary btn-xs"> Testimonials Details</a> </div>
                <br/>
                <h4><i class="icon-th-list"></i> Id #: @Request.QueryString["id"] | Received Date: @ViewBag.date </h4>
            </div>
            <br/>
        </div>
            <!-- Tabls Starts here -->
@foreach (var item in ViewBag.Testimonal)
{
            <div class="col-lg-10">
                <div class="panel panel-primary">
                    <div class="panel-heading"> Customer Information </div>
                    <div class="panel-body">
                        <p><b>Customer Id :</b> @item.UpdatedBy </p>
                        <p><b>Customer Name :</b> @item.FirstName @item.LastName </p>
                        <p><b>Email:</b> @item.UserName  @*<a href="#" class="btn btn-primary btn-sm">Verified</a>*@</p>
                        <p><b>Phone:</b> @item.UserPhone @*<a href="#" class="btn btn-primary btn-sm">Not Verified</a>*@</p>
                        <p><b>Address:</b> @item.Address</p>
                    </div>
                </div>
            </div>
            <!-- Tabs Ends here -->
        
        <div class="col-lg-10">
            <div class="panel panel-primary">
                <div class="panel-heading"> FeedBack </div>
                <div class="panel-body">
                    <p><b>Description:</b> @item.Description </p>
                    <p><b>Rating:</b> 4 </p>
                    <p><b>Images:</b> 
                        <div class="studentPic-inner">
                            <div>
                                @foreach (var item1 in ViewBag.images)
                                {
                                    <img class="lazy" src="~/Testimonial/@item1" alt="student" style="height:100px;width:150px;border:double">
                                }
                            </div>
                        </div>
                    </p>
                    @*<p><b>Videos:</b> https://i1.ytimg.com/vi/8jKYP24uhzI/maxresdefault.jpg </p>*@
                </div>
            </div>
            <form method="post">
                <div align="center">
                <input type="submit" value="Approve" name="command" class="btn btn-success" style="width:150px" />
                <input type="submit" value="Cancel" name="command" class="btn btn-danger" style="width:150px" />
                </div>
            </form>
        </div>
        }
        
        @*<div class="col-lg-6">
                <div class="panel panel-primary">
